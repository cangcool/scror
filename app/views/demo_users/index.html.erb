<h1>Listing demo_users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demo_users.each do |demo_user| %>
      <tr>
        <td><%= demo_user.username %></td>
        <td><%= demo_user.email %></td>
        <td><%= demo_user.created_at %></td>
        <td><%= link_to 'Show', demo_user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demo user', new_demo_user_path %>
