<h1>Listing sprint_task_types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sprint</th>
      <th>Color code</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sprint_task_types.each do |sprint_task_type| %>
      <tr>
        <td><%= sprint_task_type.name %></td>
        <td><%= sprint_task_type.sprint_id %></td>
        <td><%= sprint_task_type.color_code %></td>
        <td><%= sprint_task_type.user_id %></td>
        <td><%= link_to 'Show', sprint_task_type %></td>
        <td><%= link_to 'Edit', edit_sprint_task_type_path(sprint_task_type) %></td>
        <td><%= link_to 'Destroy', sprint_task_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sprint task type', new_sprint_task_type_path %>
