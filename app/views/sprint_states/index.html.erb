<h1>Listing sprint_states</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sprint_states.each do |sprint_state| %>
      <tr>
        <td><%= sprint_state.name %></td>
        <td><%= sprint_state.description %></td>
        <td><%= link_to 'Show', sprint_state %></td>
        <td><%= link_to 'Edit', edit_sprint_state_path(sprint_state) %></td>
        <td><%= link_to 'Destroy', sprint_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sprint state', new_sprint_state_path %>
